sudo mkdir /root/cap/pa/th/c157
sudo mkdir /root/cap/pa/th/c159
sudo mkdir /root/cap/pa/th/c161
sudo mkdir /root/cap/pa/th/c165
sudo mkdir /root/cap/pa/th/c169
sudo mkdir /root/cap/pa/th/c173

# Check the WiFi interface (usually wlp or wlan)
ip a
# Enable monitor mode, you will lose internet connectivity if you do not have another interface providing it:
airmon-ng check kill
airmon-ng start <interface>
# Ctrl + H replace <interface> with your new monitor WiFi interface ((wlp*s*mon or wlan*mon):

# Enumerating
# Seperate terminals:
cd /root/cap/pa/th/c1/
sudo airodump-ng --channel 1 -w YYMMDD-HHSS --output-format csv <interface>

cd /root/cap/pa/th/c2/
sudo airodump-ng --channel 2 -w YYMMDD-HHSS --output-format csv <interface>

